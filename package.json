{
  "name": "pyth-price-feed-optimizer",
  "version": "1.0.0",
  "description": "TypeScript implementation for decoding, filtering, and re-encoding Pyth Network price feed data for gas-optimized blockchain transactions",
  "main": "dist/encode-updates.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/encode-updates.js",
    "fetch-prices": "npm run build && node dist/fetch-prices.js"
  },
  "keywords": ["pyth", "blockchain", "defi", "price-feeds", "ethereum", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pythnetwork/hermes-client": "^1.0.0",
    "@pythnetwork/price-service-client": "^1.9.0",
    "@pythnetwork/pyth-evm-js": "^1.83.0",
    "bn.js": "^5.2.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/node": "^20.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
